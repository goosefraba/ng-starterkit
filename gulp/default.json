{
  "build": {
    "version": "SNAPSHOT",
    "src": "./src",
    "build": "./build",
    "bin": "./bin",
    "dir": {
      "assets": "assets",
      "scss": "scss",
      "less": "less",
      "css": "css"
    },
    "pattern": {
      "allSubDirs": "/**",
      "ts": "*.ts",
      "less": "*.less",
      "scss": "*.scss",
      "css": "*.css",
      "js": "*.js",
      "json": "*.json",
      "cfg": "*.json",
      "html": "*.html",
      "devSuffix": "-DEV"
    },
    "task": {
      "less": {
        "src": []
      },
      "scss": {
        "src": []
      },
      "static": {
        "src": []
      },
      "inject": {
        "target": "index.html"
      },
      "typescript": {
        "main": "app.ts",
        "outputname": "app"
      }
    }
  }
}